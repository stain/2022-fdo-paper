author = {Carriero, Valentina Anita and Daquino, Marilena and Gangemi, Aldo and Nuzzolese, Andrea Giovanni and Peroni, Silvio and Presutti, Valentina and Tomasi, Francesca},
editor = {Cota, Giuseppe and Daquino, Marilena and Pozzato, Gian Luca},
