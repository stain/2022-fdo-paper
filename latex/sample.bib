@techreport{collins_2018,
title = {Turning {FAIR} into reality: Final report and action plan from the European Commission expert group on {FAIR} data},
author = {Collins, Sandra and Genova, Françoise and Harrower, Natalie and Hodson, Simon and Jones, Sarah and Laaksonen, Leif and Mietchen, Daniel and Petrauskaité, Rūta and Wittenburg, Peter},
editor = {European Commission Expert Group on {FAIR} Data,},
