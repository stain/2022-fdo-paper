author = {Weiland, Claus and Islam, Sharif and Broder, Daan and Anders, Ivonne and Wittenburg, {PEter}},
series = {{FDO} Forum},
editor = {Weiland, Claus and Wittenburg, Peter},
publisher = {{FDO} Forum},
