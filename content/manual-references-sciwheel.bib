day = {25},
notes = {{WD-MachineActionDef-1.1-20220301}},
edition = {1.1},
address = {(internal draft)},
sciwheel-projects = {2022-fdo-comparison and Papers},
abstract = {The {FDO} Forum will refer in many of its documents to the term \textquotedblleftmachine actionability\textquotedblright which was coined in the foundational article and later exemplified by {GO} {FAIR} implementation networks in the realm of the {FAIR} principles. For the {FDO} work we need a clear definition of what is meant with the term \textquotedblleftmachine actionability\textquotedblright. This document includes a verbose description of relevant terms and draws conclusions in the form of a definition for use in the {FDO} Forum. Machine actionability, in the context of {FDO}, represents the {FDO} Forum community\textquoterights focus on architecting the {FDO} to maximize dynamic data accessibility, parsability, interpretability, understandability, and synthesizability by machines while minimizing or eliminating the need for human oversight.},
type = {Working Draft}
}
